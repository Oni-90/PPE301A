{{ form_start(form) }}
<div class="row clearfix">
    <div class="col-md-12 col-sm-12">
        <div class="form-group">
            <div class="form-line">
                {{ form_row(form.code, {'attr':{type:"text"  ,'class':"form-control" ,'placeholder':"code clase" , 'require':""}}) }}
            </div>
        </div>
    </div>
    <div class="col-md-12 col-sm-12">
        <div class="form-group">
            <div class="form-line">
                {{ form_row(form.libelle, {'attr':{'class':"form-control" ,'placeholder':"libelle" ,'require':"", 'label':'Prenom*'}}) }}
            </div>
        </div>
    </div>
</div>

    <button class="btn">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}
